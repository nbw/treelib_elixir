<html>
  <head>
    <title>Genus: <%= raw @page_data.genus.name %></title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <%# <meta property="og:url"           content="http://treelib.ca/species/genus/<%= @page_data.genus.name %1>" /> %>
    <meta property="og:type"          content="website" />
    <meta property="og:title"         content="TreeLib Genus: <%= @page_data.genus.name %>" />
    <meta property="og:description"   content="" />
  <link rel='shortcut icon' href='/img/favicon.ico' type='image/x-icon'/ >
  <link href="https://fonts.googleapis.com/css?family=Roboto:400,400i,700" rel="stylesheet">
  <link rel="stylesheet" type="text/css" href="/css/genus_page.css">
</head>
<body>
  <div class="mainContainer">
    <p class="alert alert-info" role="alert"><%= get_flash(@conn, :info) %></p>
    <p class="alert alert-danger" role="alert"><%= get_flash(@conn, :error) %></p>
    <div id="app"></div>
  </div>
  <script type="text/javascript">
      var CSRF_TOKEN = <%= raw json_encode!(Plug.CSRFProtection.get_csrf_token()) %>;
      window.pageData = <%= raw json_encode!(@page_data) %>;
  </script>
  <script src="/js/autres.bundle.js"></script> 
  <script src="/js/genus_page.js"></script>
</body>
                                                                                      </html>
